Process called "main" returns Integer:
    Let lexer be lexer_create("Return x multiplied by y")

    # Print all tokens
    Let continue_lexing be 1
    While continue_lexing is equal to 1:
        Let token be lexer_next_token(lexer)
        Let token_type be memory_get_integer(token, 0)
        print_string("Token type: ")
        print_integer(token_type)
        print_string(", value: ")
        If token_type is equal to 0:  # TOKEN_EOF
            print_string("EOF")
            Set continue_lexing to 0
        Otherwise:
            Let token_value be memory_get_pointer(token, 8)
            print_string(token_value)
        End If
        print_newline()
    End While

    lexer_destroy(lexer)
    Return 0
End Process