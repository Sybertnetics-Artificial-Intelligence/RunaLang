# String Operations Test
# Tests basic string operations using runtime functions

Process called "main" returns Integer:
    # Test string length
    Let hello be "Hello"
    Let len be string_length(hello)
    If len is not equal to 5:
        Return 1
    End If

    # Test string equality
    Let str1 be "World"
    Let str2 be "World"
    Let str3 be "world"

    If string_equals(str1, str2) is equal to 0:
        Return 2
    End If

    If string_equals(str1, str3) is not equal to 0:
        Return 3
    End If

    # Test empty string
    Let empty be ""
    Let empty_len be string_length(empty)
    If empty_len is not equal to 0:
        Return 4
    End If

    # Test string with numbers
    Let numbers be "12345"
    Let num_len be string_length(numbers)
    If num_len is not equal to 5:
        Return 5
    End If

    # Test integer to string conversion
    Let num be 42
    Let num_str be integer_to_string(num)
    If string_equals(num_str, "42") is equal to 0:
        Return 6
    End If

    # Test string to integer conversion
    Let str_num be "123"
    Let converted be string_to_integer(str_num)
    If converted is not equal to 123:
        Return 7
    End If

    Return 0
End Process