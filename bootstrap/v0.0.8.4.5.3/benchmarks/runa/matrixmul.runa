Process called "main" returns Integer:
    Let n be 100
    Let size be n multiplied by n multiplied by 8
    Let a be allocate(size)
    Let b be allocate(size)
    Let c be allocate(size)

    Let i be 0
    While i is less than n:
        Let j be 0
        While j is less than n:
            Let offset be i multiplied by n plus j
            Set offset to offset multiplied by 8
            memory_set_integer(a, offset, i plus j)
            memory_set_integer(b, offset, i minus j)
            memory_set_integer(c, offset, 0)
            Set j to j plus 1
        End While
        Set i to i plus 1
    End While

    Set i to 0
    While i is less than n:
        Let j be 0
        While j is less than n:
            Let sum be 0
            Let k be 0
            While k is less than n:
                Let offset_a be i multiplied by n plus k
                Set offset_a to offset_a multiplied by 8
                Let offset_b be k multiplied by n plus j
                Set offset_b to offset_b multiplied by 8
                Let val_a be memory_get_integer(a, offset_a)
                Let val_b be memory_get_integer(b, offset_b)
                Set sum to sum plus val_a multiplied by val_b
                Set k to k plus 1
            End While
            Let offset_c be i multiplied by n plus j
            Set offset_c to offset_c multiplied by 8
            memory_set_integer(c, offset_c, sum)
            Set j to j plus 1
        End While
        Set i to i plus 1
    End While

    deallocate(a)
    deallocate(b)
    deallocate(c)
    Return 0
End Process
