Type called "Point":
    x as Integer
    y as Integer
End Type

Type called "Rectangle":
    topLeft as Point
    bottomRight as Point
End Type

Type called "Canvas":
    bounds as Rectangle
    width as Integer
    height as Integer
End Type

Process called "main" returns Integer:
    Let canvas be Canvas
    Set canvas.bounds.topLeft.x to 10
    Set canvas.bounds.topLeft.y to 20
    Set canvas.bounds.bottomRight.x to 50
    Set canvas.bounds.bottomRight.y to 80
    Set canvas.width to 640
    Set canvas.height to 480

    Return canvas.bounds.bottomRight.x
End Process