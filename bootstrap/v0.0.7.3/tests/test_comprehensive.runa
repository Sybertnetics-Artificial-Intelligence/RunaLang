Import "utils" as Utils

Type called "Config":
    input_file as Integer,
    output_file as Integer,
    count as Integer
End Type

Process called "process_file" takes filename as Integer returns Integer:
    Let content be read_file("test_input.txt")
    Print read_file("test_input.txt")
    Return 0
End Process

Process called "main" takes argc as Integer, argv as Integer returns Integer:
    Let config be Config
    Set config.count to 42

    Let test_content be read_file("test_input.txt")
    Let write_result be write_file("test_comprehensive_output.txt", "All features working!")

    Let result be process_file(0)

    Return config.count
End Process