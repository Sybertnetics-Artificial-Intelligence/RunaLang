# Test field access: canonical natural language syntax
Type called "Rectangle":
    width as Integer
    height as Integer
End Type

Process called "main" returns Integer:
    # Create a Rectangle
    Let rect be allocate(16)

    # Set fields using memory functions
    Let set1 be memory_set_integer(rect, 0, 10)
    Let set2 be memory_set_integer(rect, 8, 20)

    # Get fields using memory functions
    Let w be memory_get_integer(rect, 0)
    Let h be memory_get_integer(rect, 8)
    Display w
    Display h

    # Calculate area
    Let area be w multiplied by h
    Display area

    Let dealloc be deallocate(rect)

    Return 0
End Process
