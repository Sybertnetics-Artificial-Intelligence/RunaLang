Process called "main" takes argc as Integer, argv as Integer returns Integer:
    Print "Testing basic list operations\n"

    Let my_list be list_create()
    list_append(my_list, 10)
    list_append(my_list, 20)
    list_append(my_list, 30)

    Print "List element at index 1: "
    Print integer_to_string(list_get(my_list, 1))
    Print "\n"

    list_set(my_list, 1, 99)
    Print "After set, element at index 1: "
    Print integer_to_string(list_get(my_list, 1))
    Print "\n"

    list_insert(my_list, 0, 5)
    Print "After insert at 0, first element: "
    Print integer_to_string(list_get(my_list, 0))
    Print "\n"

    Let removed be list_remove(my_list, 1)
    Print "Removed element: "
    Print integer_to_string(removed)
    Print "\n"

    Print "Find value 30: "
    Print integer_to_string(list_find(my_list, 30))
    Print "\n"

    list_destroy(my_list)
    Return 0
End Process