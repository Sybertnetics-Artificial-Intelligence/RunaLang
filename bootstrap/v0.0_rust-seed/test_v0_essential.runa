Process called "add_five" that takes x as Integer returns Integer:
    Return x plus 5
End Process

Process called "countdown" that takes start as Integer returns Integer:
    Let count be start
    While count is greater than 0:
        Print count
        Set count to count minus 1
    End While
    Return 0
End Process

Let result be add_five(10)
Print result

Set result to countdown(3)
Print result

