Let text be "Hello World"

Print "=== Testing length_of ==="
Let len be length_of(text)
Print len

Print "=== Testing char_at ==="
Let char0 be char_at(text, 0)
Print char0
Let char6 be char_at(text, 6)
Print char6

Print "=== Testing substring ==="
Let hello be substring(text, 0, 5)
Print hello
Let world be substring(text, 6, 5)
Print world

Print "=== Testing concat ==="
Let combined be concat("Runa", " Rocks")
Print combined

Print "=== Testing to_string ==="
Let num_str be to_string(42)
Print num_str
Let big_num_str be to_string(12345)
Print big_num_str

Print "=== All string functions work! ==="