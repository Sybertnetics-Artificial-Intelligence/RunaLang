        mov rdi, [rbp-16]
        mov rsi, [rbp-24]
        mov rdx, [rbp-32]
        mov r10, [rbp-40]

        // Execute syscall
        syscall

        // Store result from RAX
        mov [rbp-48], rax
    End Assembly
    Return result
