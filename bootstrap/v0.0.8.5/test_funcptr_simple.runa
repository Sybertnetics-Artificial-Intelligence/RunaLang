Process called "add" takes a as Integer, b as Integer returns Integer:
    Return a plus b
End Process

Process called "main" returns Integer:
    Let fn_ptr be $add
    Let result be fn_ptr(3, 4)
    If result is equal to 7:
        Display "SUCCESS"
        Return 0
    Otherwise:
        Display "FAIL"
        Return 1
    End If
End Process
