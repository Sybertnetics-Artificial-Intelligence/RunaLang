Type called "ConversionResult":
    source_uuid as String
    source_format as String
    target_uuid as String
    target_format as String
    conversion_status as String

Type called "ValidationResult":
    uuid_candidate as String
    is_valid as Boolean
    validation_errors as List[String]
    validation_warnings as List[String]
    quality_score as Float

