Import "collections" as Collections

Type called "BatteryTestSuite":
    suite_id as String
    suite_name as String
    battery_tests as List[BatteryTestCase]
    target_device as String
    baseline_consumption as Float
    test_duration as Float
    enabled as Boolean

Type called "BatteryTestCase":
    test_id as String
    test_name as String
    test_scenario as String
    component_under_test as String
    test_duration as Float
    expected_drain_rate as Float
    optimization_target as String

Type called "BatteryMetrics":
