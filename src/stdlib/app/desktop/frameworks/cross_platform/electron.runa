    app_name as String
    app_version as String
    is_ready as Boolean
    windows as List[Integer]
    user_data_path as String
    command_line_args as List[String]

Type called "BrowserWindow":
    window_id as Integer
    width as Integer
    height as Integer
    x as Integer
    y as Integer
    title as String
    url as String
    webcontents_id as Integer
    parent_id as Integer
    modal as Boolean
    fullscreen as Boolean

Type called "IpcMessage":
    channel as String
    sender_id as Integer
    args as List[String]
    reply_channel as String

Type called "MenuItem":
    label as String
    accelerator as String
    role as String
    submenu as List[MenuItem]
    enabled as Boolean
    visible as Boolean
    checked as Boolean
    click_handler as String

