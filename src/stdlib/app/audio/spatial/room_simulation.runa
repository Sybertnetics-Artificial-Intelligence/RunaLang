Note: ============================================================================
Note: DATA STRUCTURES
Note: ============================================================================

Type called "RoomGeometry":
    surfaces as List[Surface]
    dimensions as List[Float]
    volume as Float
    surface_area as Float
    shape_factor as Float
    acoustic_center as Position3D

Type called "Surface":
    vertices as List[Position3D]
    material as AcousticMaterial
    surface_type as String
    area as Float
    normal_vector as List[Float]
    reflection_coefficient as Float

Type called "AcousticMaterial":
    name as String
    absorption_coefficients as List[Float]
    scattering_coefficients as List[Float]
    frequency_bands as List[Float]
    density as Float
    sound_speed as Float
    impedance as Complex

Type called "RoomModel":
    geometry as RoomGeometry
    sources as List[AudioSource]
    listeners as List[AudioListener]
    environmental_conditions as EnvironmentalConditions
    simulation_parameters as SimulationParameters
    cache_data as Dictionary[String, Any]

Type called "EnvironmentalConditions":
    temperature as Float
    humidity as Float
    air_pressure as Float
    air_density as Float
    sound_speed as Float
    atmospheric_absorption as List[Float]

Type called "SimulationParameters":
    max_reflection_order as Integer
    ray_count as Integer
    time_resolution as Float
    frequency_resolution as Float
    simulation_duration as Float
    quality_preset as String

Type called "RoomImpulseResponse":
    impulse_data as List[Float]
    early_reflections as List[Reflection]
    late_reverberation as List[Float]
    rt60_values as List[Float]
    energy_decay_curve as List[Float]
    sample_rate as Integer

Type called "Reflection":
    arrival_time as Float
    amplitude as Float
    direction as List[Float]
    reflection_path as List[Position3D]
    reflection_order as Integer
    frequency_response as List[Float]

