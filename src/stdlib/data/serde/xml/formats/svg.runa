Note: =====================================================================

Type called "SvgDocument":
    version as String
    xmlns as String
    width as String
    height as String
    viewbox as String
    preserve_aspect_ratio as String
    elements as List[Dictionary[String, String]]
    defs as List[Dictionary[String, String]]
    styles as List[Dictionary[String, String]]
    scripts as List[String]
    metadata as Dictionary[String, String]

Type called "SvgElement":
    element_type as String
    id as String
    class_list as List[String]
    attributes as Dictionary[String, String]
    style as Dictionary[String, String]
    transform as String
    children as List[Dictionary[String, String]]
    content as String

Type called "SvgPath":
    path_data as String
    commands as List[Dictionary[String, String]]
    fill as String
    stroke as String
    stroke_width as String
    path_length as Float

Type called "SvgStyle":
    selector as String
    properties as Dictionary[String, String]
    media_queries as List[String]
    specificity as Integer

