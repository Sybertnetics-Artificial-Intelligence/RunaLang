Note: =====================================================================

Type called "RssFeed":
    version as String
    title as String
    description as String
    link as String
    language as String
    copyright as String
    pub_date as String
    last_build_date as String
    generator as String
    docs as String
    cloud as Dictionary[String, String]
    ttl as Integer
    image as Dictionary[String, String]
    rating as String
    skip_hours as List[Integer]
    skip_days as List[String]
    categories as List[Dictionary[String, String]]
    managing_editor as String
    web_master as String

Type called "RssItem":
    title as String
    description as String
    link as String
    author as String
    category as List[Dictionary[String, String]]
    comments as String
    enclosure as Dictionary[String, String]
    guid as Dictionary[String, String]
    pub_date as String
    source as Dictionary[String, String]
    content_encoded as String
    dublin_core as Dictionary[String, String]

Type called "RssChannel":
    feed_info as RssFeed
    items as List[RssItem]
    item_count as Integer
    total_size as Integer
    last_updated as Integer
    encoding as String

Type called "RssEnclosure":
    url as String
    length as Integer
    type as String
    duration as String
    bitrate as Integer
    sample_rate as Integer

