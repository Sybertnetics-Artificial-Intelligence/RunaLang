Note: =====================================================================

Type called "TomlDocument":
    tables as Dictionary[String, Dictionary[String, String]]
    inline_tables as Dictionary[String, Dictionary[String, String]]
    arrays as Dictionary[String, List[Dictionary[String, String]]]
    array_tables as Dictionary[String, List[Dictionary[String, String]]]
    root_values as Dictionary[String, Dictionary[String, String]]
    comments as Dictionary[String, String]
    version as String

Type called "TomlTable":
    name as String
    is_inline as Boolean
    is_array_table as Boolean
    values as Dictionary[String, Dictionary[String, String]]
    nested_tables as Dictionary[String, Dictionary[String, String]]
    parent_table as String
    line_number as Integer

Type called "TomlValue":
    value_type as String
    raw_value as String
    parsed_value as String
    is_quoted as Boolean
    is_multiline as Boolean
    format_hint as String
    comment as String

Type called "TomlArray":
    element_type as String
    elements as List[TomlValue]
    is_inline as Boolean
    is_multiline as Boolean
    homogeneous as Boolean
    nested_level as Integer

