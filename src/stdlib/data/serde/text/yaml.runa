Note: YAML DOCUMENT DATA STRUCTURES
Note: =====================================================================

Type called "YamlDocument":
    version as String
    directives as Dictionary[String, String]
    content as Dictionary[String, String]
    anchors as Dictionary[String, Dictionary[String, String]]
    tags as Dictionary[String, String]
    document_markers as Dictionary[String, Boolean]
    encoding as String
    line_breaks as String

Type called "YamlNode":
    node_type as String
    value as String
    tag as String
    anchor as String
    alias as String
    children as List[Dictionary[String, String]]
    properties as Dictionary[String, String]
    line_number as Integer
    column_number as Integer

Type called "YamlScalar":
    value as String
    style as String
    tag as String
    anchor as String
    is_plain as Boolean
    is_quoted as Boolean
    quote_style as String

Type called "YamlSequence":
    items as List[YamlNode]
    style as String
    tag as String
    anchor as String
    flow_style as Boolean
    block_style as Boolean

Type called "YamlMapping":
    pairs as List[Dictionary[String, YamlNode]]
    style as String
    tag as String
    anchor as String
    flow_style as Boolean
    block_style as Boolean

