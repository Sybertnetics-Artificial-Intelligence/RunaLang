Note:
data/serde/text/ini.runa
INI Configuration Format Processing

This module provides comprehensive INI file capabilities including
INI parsing, generation, section management, key-value handling,
comment processing, and multi-format INI dialect support.
:End Note

Import "dev/debug/errors/core" as Errors

Note: =====================================================================
Note: INI DOCUMENT DATA STRUCTURES
Note: =====================================================================

Type called "IniDocument":
    sections as Dictionary[String, Dictionary[String, String]]
    global_properties as Dictionary[String, String]
    comments as Dictionary[String, String]
    section_order as List[String]
    case_sensitive as Boolean
    allow_duplicates as Boolean
    delimiter_style as String
    encoding as String

Type called "IniSection":
    name as String
    properties as Dictionary[String, String]
    comments as Dictionary[String, String]
    property_order as List[String]
    line_number as Integer
    is_case_sensitive as Boolean

Type called "IniProperty":
    key as String
    value as String
    comment as String
    delimiter as String
    is_quoted as Boolean
    line_number as Integer
    value_type as String

Type called "IniParsingOptions":
    case_sensitive_keys as Boolean
    case_sensitive_sections as Boolean
    allow_duplicate_keys as Boolean
    allow_duplicate_sections as Boolean
    comment_prefixes as List[String]
    key_value_delimiter as String
    preserve_whitespace as Boolean
    interpolation_enabled as Boolean

