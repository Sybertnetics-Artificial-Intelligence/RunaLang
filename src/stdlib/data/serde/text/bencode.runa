Note:
data/serde/text/bencode.runa
BitTorrent Encoding (Bencode) Processing

This module provides comprehensive Bencode capabilities including
Bencode parsing, generation, torrent file handling, integer encoding,
string encoding, list and dictionary processing per BitTorrent specification.
:End Note

Import "dev/debug/errors/core" as Errors

Note: =====================================================================
Note: BENCODE DATA STRUCTURES
Note: =====================================================================

Type called "BencodeValue":
    value_type as String
    raw_data as String
    parsed_value as String
    byte_length as Integer
    start_position as Integer
    end_position as Integer

Type called "BencodeInteger":
    value as Integer
    is_negative as Boolean
    encoded_form as String
    byte_representation as String

Type called "BencodeString":
    value as String
    byte_length as Integer
    is_binary as Boolean
    encoding as String
    encoded_form as String

Type called "BencodeList":
    elements as List[BencodeValue]
    element_count as Integer
    total_byte_length as Integer
    encoded_form as String

Type called "BencodeDictionary":
    entries as Dictionary[String, BencodeValue]
    key_order as List[String]
    entry_count as Integer
    total_byte_length as Integer
    encoded_form as String

Type called "BencodeDocument":
    root_value as BencodeValue
    total_size as Integer
    encoding_errors as List[String]
    metadata as Dictionary[String, String]

