Note: RON DOCUMENT DATA STRUCTURES
Note: =====================================================================

Type called "RonDocument":
    root_value as Dictionary[String, String]
    extensions as List[String]
    comments as Dictionary[String, String]
    formatting_style as String
    decimal_separator as String
    thousand_separator as String

Type called "RonValue":
    value_type as String
    raw_value as String
    parsed_value as String
    type_annotation as String
    is_named as Boolean
    field_name as String

Type called "RonStruct":
    struct_name as String
    fields as Dictionary[String, RonValue]
    is_named_fields as Boolean
    is_tuple_struct as Boolean
    field_order as List[String]

Type called "RonEnum":
    variant_name as String
    variant_type as String
    variant_data as Dictionary[String, String]
    is_unit_variant as Boolean
    is_tuple_variant as Boolean
    is_struct_variant as Boolean

Type called "RonArray":
    elements as List[RonValue]
    element_type as String
    is_homogeneous as Boolean
    bracket_style as String

Type called "RonMap":
    entries as Dictionary[RonValue, RonValue]
    key_type as String
    value_type as String
    bracket_style as String

