    dimensions as List[String]
    measures as List[String]
    hierarchies as Dictionary[String, List[String]]
    aggregation_functions as Dictionary[String, String]
    time_dimension as String
    storage_type as String
    compression_enabled as Boolean

Type called "Dimension":
    dimension_name as String
    dimension_type as String
    hierarchy_levels as List[String]
    level_attributes as Dictionary[String, List[String]]
    dimension_table as String
    key_column as String
    name_column as String
    parent_column as String

Type called "Measure":
    measure_name as String
    data_type as String
    aggregation_function as String
    format_string as String
    calculation_expression as String
    is_calculated as Boolean
    dependency_measures as List[String]

Type called "OlapQuery":
    select_dimensions as List[String]
    select_measures as List[String]
    filter_conditions as Dictionary[String, String]
    drill_down_path as List[String]
    slice_criteria as Dictionary[String, String]
    dice_criteria as Dictionary[String, List[String]]
    pivot_configuration as Dictionary[String, String]

