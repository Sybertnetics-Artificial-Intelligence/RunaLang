Note:
data/database/search/indexing.runa
Generic Search Indexing Framework

This module provides comprehensive search indexing capabilities including
index creation, document processing, field mapping, batch indexing,
incremental updates, and index management across different search engines.
:End Note

Import "dev/debug/errors/core" as Errors

Note: =====================================================================
Note: SEARCH INDEXING DATA STRUCTURES
Note: =====================================================================

Type called "SearchIndexConfig":
    index_name as String
    index_type as String
    storage_path as String
    analyzer_type as String
    field_mappings as Dictionary[String, String]
    settings as Dictionary[String, String]
    shards as Integer
    replicas as Integer

Type called "IndexDocument":
    document_id as String
    document_type as String
    fields as Dictionary[String, String]
    metadata as Dictionary[String, String]
    boost_factor as Float
    timestamp as Integer

Type called "FieldMapping":
    field_name as String
    field_type as String
    analyzer as String
    stored as Boolean
    indexed as Boolean
    boost_factor as Float
    multi_value as Boolean

Type called "IndexBatch":
    batch_id as String
    documents as List[IndexDocument]
    batch_size as Integer
    processing_options as Dictionary[String, String]
    commit_immediately as Boolean

