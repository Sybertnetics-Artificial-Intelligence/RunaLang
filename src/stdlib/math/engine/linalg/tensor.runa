        Let diag_val be diagonal.get(i)
        Let vec_val be vector.get(i)
        
        Let mult_result be MathOps.multiply(diag_val, vec_val, 15)
        If mult_result.operation_successful:
            Call result.add(mult_result.result_value)
        Otherwise:
