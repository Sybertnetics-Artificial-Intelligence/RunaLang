        For Each binding_key in Dictionary.keys(impl.associated_type_bindings):
            Let binding_value be Dictionary.get(impl.associated_type_bindings, binding_key)
            Dictionary.set(result, binding_key, binding_value)
        End For
    End For
    
    Return result

