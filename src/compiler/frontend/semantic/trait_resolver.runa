    @End Implementation
    
    Let methods be List[String]
    Let lines be StringPrimitive.split(body, ";")
    
    For Each line in lines:
        Let trimmed be StringPrimitive.trim(line)
        If StringPrimitive.starts_with(trimmed, "fn"):
            List.add(methods, trimmed)
        End If
