    End If
    
    Note: Check for lowercase letter (a-z)
    If char_code is greater than or equal to 97 and char_code is less than or equal to 122:
        Return true
    End If
    
    Note: Check for underscore
    If char_code equals 95:
        Return true
    End If
    
    Return false

