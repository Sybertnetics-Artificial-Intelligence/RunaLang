{
  "name": "Runa Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1e1e1e",
    "editor.foreground": "#d4d4d4",
    "activityBarBadge.background": "#007acc",
    "sideBarTitle.foreground": "#bbbbbb"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "comment.line.note.runa",
        "comment.inline.note.runa",
        "comment.block.note.runa"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6A9955"
      }
    },
    {
      "name": "String",
      "scope": [
        "string.quoted.double.runa",
        "string.quoted.single.runa",
        "string.quoted.double.formatted.runa",
        "string.quoted.single.formatted.runa"
      ],
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric.decimal.runa",
        "constant.numeric.float.runa",
        "constant.numeric.hex.runa",
        "constant.numeric.binary.runa",
        "constant.numeric.octal.runa"
      ],
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "name": "Runa Keywords",
      "scope": [
        "keyword.control.runa",
        "keyword.declaration.runa",
        "keyword.control.exception.runa",
        "keyword.control.concurrency.runa",
        "keyword.modifier.runa",
        "keyword.other.runa"
      ],
      "settings": {
        "foreground": "#569cd6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Runa Primitive Types",
      "scope": [
        "support.type.primitive.runa",
        "storage.type.primitive.runa"
      ],
      "settings": {
        "foreground": "#4ec9b0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Runa Collection Types",
      "scope": [
        "support.type.collection.runa",
        "storage.type.collection.runa"
      ],
      "settings": {
        "foreground": "#dcdcaa",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Runa Generic Type Parameters",
      "scope": [
        "support.type.generic.parameter.runa"
      ],
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "name": "Runa Mathematical Symbols",
      "scope": [
        "keyword.operator.arithmetic.symbol.math.runa",
        "keyword.operator.comparison.symbol.math.runa",
        "keyword.operator.modulo.symbol.math.runa"
      ],
      "settings": {
        "foreground": "#ff6b6b",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Runa Natural Language Operators",
      "scope": [
        "keyword.operator.arithmetic.natural.runa",
        "keyword.operator.comparison.natural.runa",
        "keyword.operator.logical.natural.runa",
        "keyword.operator.membership.natural.runa",
        "keyword.operator.string.natural.runa"
      ],
      "settings": {
        "foreground": "#c586c0"
      }
    },
    {
      "name": "Runa Function Names",
      "scope": [
        "string.quoted.double.function-name.runa",
        "string.quoted.single.function-name.runa",
        "entity.name.function.runa"
      ],
      "settings": {
        "foreground": "#dcdcaa",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Runa Type Names",
      "scope": [
        "entity.name.type.runa"
      ],
      "settings": {
        "foreground": "#4ec9b0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Runa Variable Names",
      "scope": [
        "variable.name.runa",
        "variable.other.readwrite.runa"
      ],
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "name": "Runa Built-in Functions",
      "scope": [
        "support.function.builtin.runa"
      ],
      "settings": {
        "foreground": "#dcdcaa"
      }
    },
    {
      "name": "Runa Boolean Constants",
      "scope": [
        "constant.language.boolean.runa"
      ],
      "settings": {
        "foreground": "#569cd6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Runa Null Constants",
      "scope": [
        "constant.language.null.runa"
      ],
      "settings": {
        "foreground": "#569cd6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Runa Declaration Keywords",
      "scope": [
        "keyword.declaration.type.runa",
        "keyword.declaration.called.runa",
        "keyword.declaration.process.runa",
        "keyword.declaration.let.runa",
        "keyword.declaration.be.runa",
        "keyword.declaration.is.runa"
      ],
      "settings": {
        "foreground": "#c586c0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Runa Invalid/Error - Incomplete Comments",
      "scope": [
        "invalid.illegal.comment.block.note.incomplete.runa",
        "invalid.illegal.comment.block.note.incomplete.content.runa"
      ],
      "settings": {
        "foreground": "#ff6b6b",
        "background": "#4d1f1f",
        "fontStyle": "italic"
      }
    }
  ]
}